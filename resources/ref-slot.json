{
    "framework": {
        "metaData":         "/design/metaData",
        "fragmentRootDir":  "/design/fragment",
        "webRootDir":       "/www",
        "mvcRootDir":       "/backend/mvc",
        "restRootDir":      "/backend/rest",
        "dbRootDir":        "/backend/db",
        "restFilter":       "/rest",
        "mvcFilter":        "/mvc"
    },
    "fragments": {
        {@fragments@}
    },
    "logger": {
        "logFile":  "logs/slot.log",
        "maxsize":  104857600,
        "maxFiles": 10
    },
    "server": {
        "devMode":  true,
        "compress": true,
        "caching":  true,
        "cacheType":"inMemory"
    }
}
